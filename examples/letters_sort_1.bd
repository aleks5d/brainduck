// sorting letters in string
// input: one string
// possible inputs:
// abcde
// vjrkwbvlca
// qel nlqenlfq lfneqlkn lqe nlefqnl
// input must end by '\n'
cmp2:                           // x y 0 0
        >+<                     // x y+1 0 0
        [->->+<{>>-<<}<]        // 0 y+1-x x -x+(y<x)
        >>
        [<<+>+>->+<]            // x y+1 0 (y<x)
        <-<                     // x y 0 (y<x)
swap: // x y 0
        [->>+<<]                // 0 y x
        >
        [-<+>]                  // y 0 x
        >
        [-<+>]                  // y x 0
        <<
plus10:
        ++++++++++
minus10:
        ----------
mov2r:
        [->>+<<]
mov2l:
        [-<<+>>]
readString:
        ,minus10|[plus10|>,minus10|]
writeString:
        [.>]
main:
        >readString|<
        mov2r|<
        [
                >>>mov2l|<<<
                cmp2| // x y 0 (y < x)
                >>>
                {
                        -<<<swap| // y x 0 0
                        >>>>[mov2l|>]
                        <<
                        <mov2r|
                        -
                }
                +
                {
                        -
                        <<mov2r|
                        <mov2r|
                }
                <
        ]
        >>>writeString|n|
